    "codecov": "^2.3.1",
    "create-react-class": "^15.6.3",
    "dirty-chai": "^2.0.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-xurei": "^1.0.3",
    "eslint-plugin-jsx": "0.0.2",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-security": "^1.3.0",
    "isparta": "^4.0.0",
    "jsdom": "^11.10.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.5.3",
    "react": "^15.6.2",
    "react-addons-test-utils": "^15.6.2",
