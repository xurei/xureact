{
  "name": "xureact",
  "version": "1.1.0",
  "description": "React toolbox",
  "main": "lib/index.js",
  "scripts": {
    "pretest": "node_modules/.bin/eslint .",
    "eslint": "node_modules/.bin/eslint .",
    "test": "./node_modules/.bin/mocha --compilers js:@babel/register tests",
    "coverage": "(rm -R node_modules/isparta/node_modules/babel-core || exit 0) && node_modules/.bin/babel-node ./node_modules/isparta/bin/isparta cover --include 'src/**/*.js' _mocha -- --reporter dot tests",
    "postcoverage": "node_modules/.bin/codecov",
    "precompile": "npm test",
    "preversion": "npm test",
    "build": "rimraf lib/* && node_modules/.bin/babel src -d lib",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "React"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xurei/xureact.git"
  },
  "author": {
    "name": "Olivier Bourdoux",
    "email": "olivier.bourdoux@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "deep-eql": "^4.0.0",
    "prop-types": "^15.6.2"
  },
  "peerDependencies": {
    "react": ">15.0.0",
    "react-dom": ">15.0.0",
    "webpack": ">2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "blanket": "^1.2.3",
    "chai": "^4.0.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.8.0",
    "codecov": "^3.1.0",
    "create-react-class": "^15.6.3",
    "dirty-chai": "^2.0.1",
    "enzyme": "^2.8.2",
    "eslint": "^5.12.0",
    "eslint-config-xurei": "^1.1.0",
    "eslint-plugin-jsx": "0.0.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-security": "^1.3.0",
    "isparta": "^4.1.1",
    "jsdom": "^13.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "react": "^15.6.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "react-tools": "^0.13.3",
    "rimraf": "^2.6.3",
    "sinon": "^2.4.1",
    "sinon-chai": "^2.14.0"
  }
}
