{
  "name": "xureact",
  "version": "1.3.4",
  "description": "React toolbox",
  "main": "lib/cjs/index.js",
  "module": "lib/module/index.js",
  "sideEffects": false,
  "scripts": {
    "pretest": "npm run eslint",
    "lint": "npm run eslint",
    "eslint": "node_modules/.bin/eslint .",
    "test": "node_modules/.bin/mocha --require @babel/register --require tests/setup.js tests",
    "coverage": "./node_modules/.bin/nyc --reporter=lcov node_modules/.bin/mocha --require @babel/register --require tests/setup.js tests",
    "postcoverage": "node_modules/.bin/codecov",
    "precompile": "npm test",
    "preversion": "npm test",
    "build": "rimraf lib/* && npm run build:module && npm run build:cjs",
    "build:module": "BABEL_ENV=module node_modules/.bin/babel src -d lib/module",
    "build:cjs": "node_modules/.bin/babel src -d lib/cjs",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "React"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xurei/xureact.git"
  },
  "author": {
    "name": "Olivier Bourdoux",
    "email": "olivier.bourdoux@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "deep-eql": "^4.0.0",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": ">15.5.0",
    "react-dom": ">15.5.0",
    "webpack": ">2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/eslint-parser": "^7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@babel/register": "^7.13.8",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "babel-core": "7.0.0-bridge.0",
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "codecov": "^3.8.1",
    "create-react-class": "^15.7.0",
    "dirty-chai": "^2.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-15": "^1.4.3",
    "eslint": "^7.21.0",
    "eslint-config-xurei": "^1.1.1",
    "eslint-plugin-jsx": "0.0.2",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-security": "^1.4.0",
    "fbjs": "^0.8.17",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "lodash": "^4.17.19",
    "mocha": "^8.3.0",
    "nyc": "^15.1.0",
    "react": "^15.7.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^15.7.0",
    "react-test-renderer": "^15.7.0",
    "rimraf": "^2.7.1",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.5.0"
  }
}
