    "preversion": "npm test",
    "compile": "rimraf lib/* && babel src -d lib",
    "prepublishOnly": "npm run compile"
  },
  "keywords": [
    "React"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xurei/xureact.git"
